{
  "name": "sta-lambda",
  "version": "1.0.0",
  "description": "Smart Tags Analytics Lambda Functions for Blockchain and Pinata services",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs": "serverless logs -f",
    "invoke": "serverless invoke -f",
    "test": "jest",
    "lint": "eslint src/**/*.ts --ext .ts",
    "lint:fix": "eslint src/**/*.ts --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint && npm run format:check && npm run build",
    "ci": "npm run type-check && npm run lint && npm run format:check && npm run build"
  },
  "keywords": [
    "aws",
    "lambda",
    "blockchain",
    "pinata",
    "serverless"
  ],
  "author": "Smart Tags Analytics Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.918.0",
    "@pinata/sdk": "^1.0.0",
    "axios": "^1.6.0",
    "ethers": "^6.8.1",
    "idempotent-babel-polyfill": "^7.4.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "serverless": "^3.35.0",
    "serverless-esbuild": "^1.51.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
